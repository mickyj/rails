<%= form_for @painting do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <br>
  <%= f.label :medium %>
  <%= f.text_field :medium %>
  <br>
  <%= f.label :stlye %>
  <%= f.text_field :style %>
  <br>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <br>
  <%= f.label :artist_id %>
  <%= f.select :artist_id, Artist.all.map {|artist| [artist.name, artist.id]} %>
  <br>
  <%= f.submit %>
  <%end%>